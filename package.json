{
  "name": "@apimatic/sdk",
  "description": "This API gives you programmatic access to APIMatic's Code Generation, Transformers Engine and Docs Generation.",
  "author": "Team APIMatic",
  "version": "0.0.1-alpha.10",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/sdk.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "apimatic",
    "sdk",
    "api"
  ],
  "scripts": {
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src",
    "prepare": "husky install"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "husky": "^7.0.4",
    "stream-equal": "^2.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@apimatic/core": "^0.7.2",
    "@apimatic/schema": "^0.6.0"
  },
  "bugs": {
    "email": "contactus@apimatic.io"
  }
}
